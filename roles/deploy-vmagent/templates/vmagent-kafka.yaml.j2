global:
   scrape_interval:
scrape_configs:
   - job_name: vmagent
     static_configs:
     - targets: [{{ inventory_hostname }}:8429]
   - job_name: node_exporter
     static_configs:
     - targets: [{{ inventory_hostname }}:9100]
   - job_name: kafka_jmx_exporter
     static_configs:
     - targets: [{{ inventory_hostname }}:8080]
   - job_name: zookeeper_jmx_exporter
     static_configs:
     - targets: [{{ inventory_hostname }}:8079]
   - job_name: schema-registry_jmx_exporter
     static_configs:
     - targets: [{{ inventory_hostname }}:8078]

